{
  "name": "qr-lambda",
  "version": "1.0.0",
  "description": "Converts content into a QRCode image",
  "main": "handler.js",
  "scripts": {
    "index": "NODE_ENV=dev node index.js",
    "index_win": "set NODE_ENV=dev&&node index.js",
    "start": "NODE_ENV=dev serverless offline start",
    "start_win": "set NODE_ENV=dev&&serverless offline start",
    "test": "NODE_ENV=test jest --setupFiles dotenv/config",
    "deploy": "NODE_ENV=dev serverless deploy --stage dev",
    "remove": "NODE_ENV=dev serverless remove --stage dev"
  },
  "author": "Silicon Jungles",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.24",
    "@types/jest": "^24.0.16",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "eslint": "^6.4.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^4.10.6",
    "serverless-plugin-include-dependencies": "^3.2.1",
    "serverless-plugin-typescript": "^1.1.7",
    "ts-jest": "^24.0.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "@typescript-eslint/parser": "^2.3.1",
    "aws-sdk": "^2.528.0",
    "axios": "^0.21.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jest": "^24.0.2",
    "qr-image": "^3.2.0",
    "qrcode": "^1.4.2"
  }
